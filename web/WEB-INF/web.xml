<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>Admincheckservlet</servlet-name>
        <servlet-class>mypackage.Admincheckservlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AlotRoomServlet</servlet-name>
        <servlet-class>mypackage.AlotRoomServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddPatient</servlet-name>
        <servlet-class>mypackage.AddPatient</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddDoctor</servlet-name>
        <servlet-class>mypackage.AddDoctor</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Doctorcheckservlet</servlet-name>
        <servlet-class>mypackage.Doctorcheckservlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Patientcheckservlet</servlet-name>
        <servlet-class>mypackage.Patientcheckservlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Appointment</servlet-name>
        <servlet-class>mypackage.Appointment</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>prescriptionservlet</servlet-name>
        <servlet-class>mypackage.prescriptionservlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MyServelet</servlet-name>
        <servlet-class>MyServelet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AdmitPatient</servlet-name>
        <servlet-class>mypackage.AdmitPatient</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Admincheckservlet</servlet-name>
        <url-pattern>/Admincheckservlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AlotRoomServlet</servlet-name>
        <url-pattern>/AlotRoomServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddPatient</servlet-name>
        <url-pattern>/AddPatient</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddDoctor</servlet-name>
        <url-pattern>/AddDoctor</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Doctorcheckservlet</servlet-name>
        <url-pattern>/Doctorcheckservlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Patientcheckservlet</servlet-name>
        <url-pattern>/Patientcheckservlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Appointment</servlet-name>
        <url-pattern>/Appointment</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>prescriptionservlet</servlet-name>
        <url-pattern>/prescriptionservlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MyServelet</servlet-name>
        <url-pattern>/MyServelet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AdmitPatient</servlet-name>
        <url-pattern>/AdmitPatient</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>
</web-app>
